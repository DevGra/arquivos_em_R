# librarys usadas para fazer o download dos links do inep
# General-purpose data wrangling
library(tidyverse)  
# Parsing of HTML/XML files  
library(rvest)    
# String manipulation
library(stringr)   
# Verbose regular expressions
library(rebus)     
# Eases DateTime manipulation
library(lubridate)

# para colorir a saida das mensagens no console 
library(crayon)

#para fazer downloads
library(downloader)